{
  "name": "xlf-translator",
  "version": "0.0.0",
  "license": "MIT",
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "electron": "ng build && electron .",
    "electron-aot": "ng build --prod && electron .",
    "package:osx": "electron-packager ./dist --platform=darwin --out=packages && cd packages && zip -9 -r omnixlf-${npm_package_version}-darwin.zip OmniXLF-darwin-x64",
    "package:windows": "electron-packager ./dist --platform=win32 --arch=ia32 --out=packages && cd packages && zip -9 -r omnixlf-${npm_package_version}-win32.zip ./OmniXLF-win32-ia32",
    "package:linux": "electron-packager ./dist --platform=linux --arch=x64 --out=packages && cd packages && tar -czf omnixlf-${npm_package_version}-linux-x64.tar.gz ./OmniXLF-linux-x64"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.9",
    "@angular/cdk": "^5.2.4",
    "@angular/common": "^5.2.0",
    "@angular/compiler": "^5.2.0",
    "@angular/core": "^5.2.0",
    "@angular/forms": "^5.2.0",
    "@angular/http": "^5.2.0",
    "@angular/material": "^5.2.4",
    "@angular/platform-browser": "^5.2.0",
    "@angular/platform-browser-dynamic": "^5.2.0",
    "@angular/router": "^5.2.0",
    "core-js": "^2.4.1",
    "dexie": "^2.0.2",
    "hammerjs": "^2.0.8",
    "humane-js": "^3.2.2",
    "material-design-icons": "^3.0.1",
    "ngx-electron": "^1.0.4",
    "rxjs": "^5.5.8",
    "web-animations-js": "^2.3.1",
    "xml2js": "^0.4.19",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@angular/cli": "~1.7.4",
    "@angular/compiler-cli": "^5.2.0",
    "@angular/language-service": "^5.2.0",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "^4.0.1",
    "electron": "^1.8.4",
    "electron-reload": "^1.2.2",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.2",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "~2.5.3"
  }
}
